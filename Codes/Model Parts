        "from pydub import AudioSegment\n",
        "\n",
        "t1 = 0 * 1000 #Works in milliseconds\n",
        "t2 = 20 * 60 * 1000\n",
        "\n",
        "newAudio = AudioSegment.from_wav(\"marriage_conversation.wav\")\n",
        "a = newAudio[t1:t2]\n",
        "a.export(\"marriage_conversation1.wav\", format=\"wav\") \n"
