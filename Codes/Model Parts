      "cell_type": "code",
      "source": [
        "audio = AudioSegment.from_wav(\"marriage_conversation1.wav\")\n",
        "spacermilli = 2000\n",
        "spacer = AudioSegment.silent(duration=spacermilli)\n",
        "audio = spacer.append(audio, crossfade=0)\n",
        "\n",
        "audio.export('audio.wav', format='wav')"
